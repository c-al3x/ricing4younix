def_key "h"
  previous_column
def_key "j"
  scroll_down
def_key "k"
  scroll_up
def_key "l"
  next_column
def_key "L"
  show_lyrics
def_key "="
  volume_up
def_key "+"
  show_clock
def_key "g"
  page_up
def_key "G"
  page_down
def_key "a"
  add_selected_items
def_key "d"
  delete_playlist_items
def_key "^"
  toggle_crossfade
def_key "&"
  set_crossfade
def_key "D"
  delete_stored_playlist
